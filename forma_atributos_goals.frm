Public Sub carga_agente()
On Error Resume Next

Dim sSelect As String

grabaahuevo = 0
   
Dim Rs As ADODB.Recordset
Set Rs = New ADODB.Recordset
    
    sSelect = "Select idemppayrolllink, idemployeeLAE, idpaytype, initials, manager, MTY, Commercial, PhoneSales, Exception1,Typeofexception1," & _
    "Exception2,Typeofexception2,Exception3,Typeofexception3,Exception4,Typeofexception4,Exception5,Typeofexception5,deb_mod, nb_deb_mod, porc_mod, autorizado, " & _
    "office1, office2, emp.firstname, emp.lastname1, managercommercial, export from [payrollconfig] payroll " & _
    "inner join EmployeeInfo emp on emp.IDEmployee=payroll.IdEmployeeLAE " & _
    "where emp.username='" + txtagent.Text + "'"


     

    ' Abrir el recordset de forma sin especificar, porque vamos a cambiar datos
    Rs.Open sSelect, base, adOpenUnspecified
        
     ' Permitir redimensionar las columnas
    Grid2.AllowUserResizing = flexResizeColumns

    ' Asignar el recordset al FlexGrid
    Set Grid2.DataSource = Rs
                         
    Rs.Close
    
    
      Grid2.row = 2
      
      Grid2.col = 1
      id$ = Grid2.Text
      If Val(id$) > 0 Then
        txtid.Text = id$
      Else
        txtid.Text = ""
      End If
      
      Grid2.col = 2
      id_LAE$ = Grid2.Text
      If Val(id_LAE$) > 0 Then
        txtid_lae.Text = id_LAE$
      Else
        txtid_lae.Text = ""
      End If
      
    
      Grid2.col = 3
      idpaytype$ = Grid2.Text
      If Grid2.Text = "idpaytype" Then
         idpaytype$ = ""
      End If
      
            
      For Y = 0 To cbotier.ListCount - 1
         If Val(Left(cbotier.List(Y), 2)) = Val(idpaytype$) Then
               cbotier.ListIndex = Y
               Exit For
         End If
      Next Y
      
      
      
      Grid2.col = 4
      txtinitials.Text = Grid2.Text
      If Grid2.Text = "initials" Then
         txtinitials.Text = ""
      End If
      
      
      Grid2.col = 5
      manager1 = Val(Grid2.Text)
      
      Grid2.col = 6
      monterrey1 = Val(Grid2.Text)
      
      Grid2.col = 7
      Commercial1 = Val(Grid2.Text)
      
      Grid2.col = 8
      phonesales1 = Val(Grid2.Text)
      
      Grid2.col = 27
      Manager_Commercial1 = Val(Grid2.Text)
      
      Grid2.col = 28
      v$ = Grid2.Text
      If v$ = "" Or v$ = "False" Then
         chk_export.Value = 0
      Else
         chk_export.Value = 1
      End If
      
      
      cbotier.Enabled = True
      
      salida = 99
      If manager1 = 1 Then
         op1(0).Value = True
      ElseIf Commercial1 = 1 Then
         op1(1).Value = True
      ElseIf monterrey1 = 1 Then
         op1(2).Value = True
         cbotier.Enabled = False
      ElseIf phonesales1 = 1 Then
         op1(4).Value = True
      ElseIf Manager_Commercial1 = 1 Then
         op1(5).Value = True
         
      ElseIf Manager_Commercial1 = 2 Then
         op1(6).Value = True
         
      Else
         op1(3).Value = True
      End If
      salida = 0
      
      
      
      
      
